.at-list {
  /* prettier-ignore */
  border-top: 1PX #e2ecf4 solid;
  position: relative;
  background-color: white; }
  .at-list::after {
    content: " ";
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
    background-color: #e2ecf4;
    /* prettier-ignore */
    height: 1PX; }
  .at-list::before {
    content: " ";
    left: 0;
    width: 24rpx;
    z-index: 100;
    position: absolute;
    background-color: transparent;
    /* prettier-ignore */
    height: calc(100% - 1PX);
    top: 0; }
  .at-list--no-border {
    border-top: none; }
    .at-list--no-border::after, .at-list--no-border::before {
      content: initial; }
